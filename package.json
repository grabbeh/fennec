{
  "name": "fennec",
  "dependencies": {
    "express": "4.0.x",
    "underscore": "~1.5.2",
    "mongoose": "3.8.5",
    "body-parser": "~1.0.1",
    "serve-static": "~1.0.4",
    "excel": "~0.1.1",
    "connect-multiparty": "~1.0.3",
    "multiparty": "~3.2.4",
    "async": "~0.7.0",
    "browserify": "~2.35.2",
    "mongoosastic": "~0.2.6",
    "moment": "~2.6.0",
    "uglify-js": "~2.4.13",
    "agenda": "~0.6.12",
    "sendgrid": "~1.0.3",
    "mustache": "~0.8.1",
    "bcrypt": "~0.8.2",
    "knox": "~0.9.0",
    "mime": "~1.2.11",
    "jsonwebtoken": "5.0.0",
    "express-jwt": "~0.4.1",
    "deep-diff": "~0.2.0",
    "angular": "1.4.0",
    "angular-route": "1.4.0",
    "angular-animate": "1.4.0",
    "angular-touch": "1.4.0"
  },
  "devDependencies": {
    "watchify": "~0.1.0",
    "catw": "~0.0.1",
    "clean-css": "2.2.14"
  },
  "scripts": {
    "build-js": "browserify client/vendor/config.js client/angular/*.js client/views/*/*.js client/modals/*/*.js | uglifyjs -c > client/production/bundle.js",
    "build-css": " cat client/css/*.css | cleancss > client/production/bundle.css",
    "build": "npm run build-js && npm run build-css",
    "watch-js": "watchify client/vendor/config.js client/angular/*.js client/views/*/*.js client/modals/*/*.js -o client/production/bundle.js -dv",
    "watch-css": "catw client/css/*.css -o client/production/bundle.css -dv",
    "watch": "npm run watch-js & npm run watch-css",
    "develop": "npm run watch & node app.js",
    "develop-vps": "npm run watch & node fennec-app.js"
  },
  "engines": {
    "node": "v0.10.22"
  },
  "version": "0.0.0-01"
}
